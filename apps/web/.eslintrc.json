{
  "extends": "../../.eslintrc.json",
  "parserOptions": {
    "projectService": true
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true,
        "project": "./tsconfig.json"
      }
    }
  },
  "rules": {
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["^leaflet$"]
      }
    ]
  }
}
