{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://deepfriedcyber.github.io/property-portal",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --config ../../eslint.config.js .",
    "build:static": "cross-env NODE_ENV=production next build",
    "export": "cross-env NODE_ENV=production next build && next export",
    "deploy": "npm run build:static && gh-pages -d out",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:a11y": "cypress run --spec \"cypress/e2e/accessibility.cy.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .next out dist coverage"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.19.2",
    "@sentry/nextjs": "^9.19.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@types/react": "^18.2.46",
    "@types/uuid": "^10.0.0",
    "@your-org/db": "workspace:*",
    "@your-org/ui": "workspace:*",
    "clsx": "^2.1.1",
    "csv-parse": "^5.6.0",
    "framer-motion": "^12.12.1",
    "logrocket": "^10.0.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@next/eslint-plugin-next": "^14.1.0",
    "@testing-library/cypress": "^10.0.1",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.10.6",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "cypress-axe": "^1.5.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.0",
    "vitest": "^3.1.3"
  }
}
