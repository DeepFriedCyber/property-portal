# Database connection string
DATABASE_URL=postgres://username:password@localhost:5432/property_portal

# Authentication (Clerk)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_publishable_key
CLERK_SECRET_KEY=your_secret_key

# Clerk URLs
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/agent/dashboard
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/agent/dashboard

# API Keys
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret
NEXT_PUBLIC_MAPBOX_TOKEN=your-mapbox-token

# Storage
UPLOAD_BUCKET=your-upload-bucket-name
STORAGE_REGION=your-storage-region
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Feature Flags
ENABLE_FEATURE_X=false
ENABLE_FEATURE_Y=false

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development