# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/property_portal

# Authentication - Clerk (get from https://clerk.com dashboard)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_PRIVATE_KEY=your_clerk_private_key_here

# File Storage - Cloudinary (get from https://cloudinary.com dashboard)
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_PUBLIC_KEY=your_cloudinary_public_key_here
CLOUDINARY_PRIVATE_KEY=your_cloudinary_private_key_here

# Redis Cache
REDIS_URL=redis://localhost:6379

# API Configuration
API_BASE_URL=http://localhost:3001
NEXT_PUBLIC_API_URL=http://localhost:3001

# Application Settings
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
LOG_LEVEL=info
